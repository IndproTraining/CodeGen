        function ReceiveCarouselImages(result) {
            var image = result.substring(0, result.length - 1)
            imagesplit = image.split(',');
            $('#hiddenFieldImagesSplit').val(imagesplit);
            for (var i = 0 ; i < imagesplit.length ; i++) {
                $("<li data-target='#myCarousel' data-slide-to=" + i + "></li>").appendTo('.carousel-indicators');
                $('<div class="item" runat="server"><img src="Downloads/Images/' + imagesplit[i] + '"  style="width: 820px; height: 150px; "></div>').appendTo('.carousel-inner');
            }
            $('.carousel-indicators > li').first().addClass('active');
            $('.item').first().addClass('active');
            $('#myCarousel').carousel({
                interval: 2000,
                cycle: true
            });
            $('#destroycar').show();
            $('#pre').show();
            $('#nxt').show();
            $('#btnCarousel').prop('disabled', true);
            return false;
        }

