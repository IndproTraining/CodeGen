                $('#li2').removeClass('active');
                $e.preventDefault();
                $('#li4').addClass('active');
                $('#ContentArea').load("Downloads/" + page4 + ".html");
            });

            $.cookie("cookiepage1", page1, { path: '/' });
            $.cookie("cookiepage2", page2, { path: '/' });
            $.cookie("cookiepage3", page3, { path: '/' });
            $.cookie("cookiepage4", page4, { path: '/' });
